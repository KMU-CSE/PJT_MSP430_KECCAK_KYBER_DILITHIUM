         mov    #3329, &OP2
                  
         sub   &RESHI, mul_res_hi 
         
         mov    src1, src2
         add    mul_res_hi, src1  
         sub    mul_res_hi, src2  

         endm
         
butter_fly_imd      macro   src1, src2, zeta

         mov    zeta, &MPYS 
         mov    src2, &OP2      
         
         mov    &RESHI, mul_res_hi                                  
         
         mov    &RESLO, &MPYS              
         mov    #-3327, &OP2 
                  
         mov    &RESLO, &MPYS 
         mov    #3329, &OP2
                  
         sub   &RESHI, mul_res_hi 
         
         mov    src1, src2
         add    mul_res_hi, src1  
         sub    mul_res_hi, src2  

         endm
         
         
merging_2  macro   zeta1, zeta2, zeta3, off1, off2, off3, off4

         mov    off1(ptr), r4
         mov    off2(ptr), r5         

         first_butter_fly     r4, r6, zeta1, off3
         first_butter_fly     r5, r7, zeta1, off4
         
         butter_fly     r4, r5, zeta2
         butter_fly     r6, r7, zeta3
         
         mov    r4, off1(ptr)
         mov    r5, off2(ptr)
         mov    r6, off3(ptr)
         mov    r7, off4(ptr)
         
         add    #2, ptr
         endm
         
merging_3  macro   zeta1, zeta2, zeta3, zeta4, zeta5, zeta6, zeta7 

         mov    0(ptr), r4
         mov    4(ptr), r5
         mov    8(ptr), r6
         mov    12(ptr), r7                

         first_butter_fly_imd     r4, r8, zeta1, 16         
         first_butter_fly_imd     r5, r9, zeta1, 20
         first_butter_fly_imd     r6, r10, zeta1, 24
         first_butter_fly_imd     r7, r11, zeta1, 28
         
         butter_fly_imd     r4, r6, zeta2
