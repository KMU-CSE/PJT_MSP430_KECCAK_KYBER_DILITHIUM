         butter_fly_imd     r6, r7, zeta5
         butter_fly_imd     r8, r9, zeta6
         butter_fly_imd     r10, r11, zeta7
         
         mov    r4, 0(ptr)
         mov    r5, 4(ptr)
         mov    r6, 8(ptr)
         mov    r7, 12(ptr)
         mov    r8, 16(ptr)
         mov    r9, 20(ptr)
         mov    r10, 24(ptr)
         mov    r11, 28(ptr)
         
         add    #2, ptr
         endm


