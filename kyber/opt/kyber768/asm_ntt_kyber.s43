mont      macro   mul_low, mul_hi
                                                     
                 
         mov    mul_low, &MPYS              
         mov    QINV, &OP2 
                  
         mov    &RESLO, &MPYS 
         mov    KYBER_Q, &OP2
                  
         sub   &RESHI, mul_hi 
         
         endm
         
mont_imd      macro   mul_low, mul_hi
                                                     
                 
         mov    mul_low, &MPYS              
         mov    #-3327, &OP2 
                  
         mov    &RESLO, &MPYS 
         mov    #3329, &OP2
                  
         sub   &RESHI, mul_hi 
         
         endm
         
first_butter_fly      macro   src1, src2, zeta, off

         mov    zeta, &MPYS 
         mov    off(ptr), &OP2       
         
         mov    &RESHI, mul_res_hi                                  
         
         mov    &RESLO, &MPYS              
         mov    QINV, &OP2 
                  
         mov    &RESLO, &MPYS 
         mov    KYBER_Q, &OP2
                  
         sub   &RESHI, mul_res_hi 
         
         mov    src1, src2
         add    mul_res_hi, src1          
         sub    mul_res_hi, src2        

         endm

         
butter_fly      macro   src1, src2, zeta

         mov    zeta, &MPYS 
         mov    src2, &OP2      
                           
         mov    &RESHI, mul_res_hi                                  
         
         mov    &RESLO, &MPYS              
         mov    QINV, &OP2 
                  
         mov    &RESLO, &MPYS 
         mov    KYBER_Q, &OP2
                  
         sub   &RESHI, mul_res_hi 
         
         mov    src1, src2
         add    mul_res_hi, src1  
         sub    mul_res_hi, src2  

         endm

first_butter_fly_imd      macro   src1, src2, zeta, off

         mov    zeta, &MPYS 
         mov    off(ptr), &OP2      
         
         mov    &RESHI, mul_res_hi                                  
         
         mov    &RESLO, &MPYS              
         mov    #-3327, &OP2 
                  
